{
  "name": "inversify-oclif-utils",
  "description": "Create oclif commands with inversify",
  "version": "1.0.0",
  "author": "Marvin Rabe",
  "homepage": "https://github.com/marvinrabe/oclif-plugin-inversify",
  "bugs": "https://github.com/marvinrabe/oclif-plugin-inversify/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "inversify": "^5",
    "inversify-inject-decorators": "^3.1.0",
    "reflect-metadata": "^0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10",
    "chai": "^4",
    "expect": "^23.6.0",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "keywords": [
    "oclif-plugin",
    "inversify",
    "typescript"
  ],
  "license": "MIT",
  "oclif": {
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/inversify.ts"
    }
  },
  "repository": "marvinrabe/oclif-plugin-inversify",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "tslint -p test -t stylish",
    "version": "oclif-dev readme && git add README.md"
  }
}
