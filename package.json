{
  "name": "inversify-oclif-utils",
  "description": "Create oclif commands with inversify",
  "version": "0.1.0",
  "author": "Marvin Rabe",
  "homepage": "https://github.com/marvinrabe/oclif-plugin-inversify",
  "bugs": "https://github.com/marvinrabe/oclif-plugin-inversify/issues",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "inversify": "^5",
    "reflect-metadata": "^0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4",
    "expect": "^23.6.0",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "keywords": [
    "oclif-plugin",
    "inversify",
    "typescript"
  ],
  "license": "MIT",
  "oclif": {
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/init.ts"
    }
  },
  "repository": "marvinrabe/oclif-plugin-inversify",
  "scripts": {
    "build": "rm -rf lib && tsc -b && oclif-dev manifest",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "postpack": "rm -f oclif.manifest.json",
    "test": "nyc mocha",
    "lint": "tslint -p test -t stylish"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
