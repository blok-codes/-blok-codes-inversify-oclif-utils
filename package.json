{
    "name": "@blok-codes/inversify-oclif-utils",
    "description": "Create oclif commands with inversify",
    "version": "1.1.0",
    "author": "Jude Seide <jude.seide@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/blok-codes/inversify-oclif-utils.git"
    },
    "bugs": {
        "url": "https://github.com/blok-codes/inversify-oclif-utils/issues"
    },
    "homepage": "https://github.com/blok-codes/inversify-oclif-utils",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "dependencies": {
        "@oclif/core": "^1.3.4",
        "inversify": "^6.0.1",
        "reflect-metadata": "^0.1.13",
        "require-main-filename": "^2.0.0",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@oclif/test": "^2.1.0",
        "@types/expect": "^1.20.3",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.18",
        "chai": "^4.3.6",
        "expect": "^27.5.1",
        "mocha": "^9.2.0",
        "nyc": "^15.1.0",
        "oclif": "^2.4.5",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "engines": {
        "node": ">=16"
    },
    "files": [
        "/lib",
        "node_modules",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/yarn.lock"
    ],
    "keywords": [
        "oclif-plugin",
        "inversify",
        "typescript"
    ],
    "license": "MIT",
    "oclif": {
        "hooks": {
            "init": "./lib/init"
        }
    },
    "scripts": {
        "build": "rm -rf lib tsconfig.tsbuildinfo && tsc -b && oclif manifest",
        "prepack": "rm -rf lib tsconfig.tsbuildinfo && tsc -b && oclif manifest",
        "postpack": "rm -f oclif.manifest.json",
        "test": "nyc mocha",
        "lint": "tslint -p test -t stylish"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json"
        ],
        "sourceMap": true,
        "instrument": true
    }
}
